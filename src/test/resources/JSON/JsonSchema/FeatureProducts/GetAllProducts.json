{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome1",
  "definitions": {
    "Welcome1": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Datum"
          }
        },
        "message": {
          "type": "string"
        },
        "status": {
          "type": "boolean"
        }
      },
      "required": [
        "data",
        "message",
        "status"
      ],
      "title": "Welcome1"
    },
    "Datum": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "user_id": {
          "type": "integer"
        },
        "category_id": {
          "type": "integer"
        },
        "product_name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "size": {
          "type": "string"
        },
        "price": {
          "type": "integer"
        },
        "stock_remaining": {
          "type": "integer"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "category": {
          "$ref": "#/definitions/CategoryClass"
        },
        "product_image": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ProductImage"
          }
        },
        "rating": {
          "type": "number"
        }
      },
      "required": [
        "category",
        "category_id",
        "description",
        "id",
        "price",
        "product_name",
        "size",
        "stock_remaining",
        "user",
        "user_id"
      ],
      "title": "Datum"
    },
    "CategoryClass": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "category": {
          "$ref": "#/definitions/CategoryEnum"
        }
      },
      "required": [
        "category"
      ],
      "title": "CategoryClass"
    },
    "ProductImage": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "image": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "https"
          ],
          "qt-uri-extensions": [
            ".png"
          ]
        }
      },
      "required": [
        "id",
        "image"
      ],
      "title": "ProductImage"
    },
    "User": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "full_name": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "shop_name": {
          "type": "string"
        },
        "photo_profile": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "https"
          ],
          "qt-uri-extensions": [
            ".png"
          ]
        }
      },
      "required": [
        "address",
        "full_name"
      ],
      "title": "User"
    },
    "CategoryEnum": {
      "type": "string",
      "enum": [
        "Sedang Tren",
        "ibu & anak",
        "Fashion"
      ],
      "title": "CategoryEnum"
    }
  }
}