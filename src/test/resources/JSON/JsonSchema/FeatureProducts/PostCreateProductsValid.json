{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome5",
  "definitions": {
    "Welcome5": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "data": {
          "$ref": "#/definitions/Data"
        },
        "message": {
          "type": "string"
        },
        "status": {
          "type": "boolean"
        }
      },
      "required": [
        "data",
        "message",
        "status"
      ],
      "title": "Welcome5"
    },
    "Data": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "user_id": {
          "type": "integer"
        },
        "category_id": {
          "type": "integer"
        },
        "product_name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "size": {
          "type": "string"
        },
        "price": {
          "type": "integer"
        },
        "stock_remaining": {
          "type": "integer"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "category": {
          "$ref": "#/definitions/Category"
        },
        "product_image": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ProductImage"
          }
        }
      },
      "required": [
        "category",
        "category_id",
        "description",
        "id",
        "price",
        "product_image",
        "product_name",
        "size",
        "stock_remaining",
        "user",
        "user_id"
      ],
      "title": "Data"
    },
    "Category": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "category": {
          "type": "string"
        }
      },
      "required": [
        "category"
      ],
      "title": "Category"
    },
    "ProductImage": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "image": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "https"
          ],
          "qt-uri-extensions": [
            ".png"
          ]
        }
      },
      "required": [
        "id",
        "image"
      ],
      "title": "ProductImage"
    },
    "User": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "full_name": {
          "type": "string"
        },
        "address": {
          "type": "string"
        }
      },
      "required": [
        "address",
        "full_name"
      ],
      "title": "User"
    }
  }
}